{
  "name": "nextjs-enterprise-boilerplate",
  "version": "1.0.1",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build:analyze": "BUNDLE_ANALYZE=both yarn next build",
    "build:css": "cd semantic-ui && yarn gulp build",
    "build:js": "next build",
    "build:storybook": "build-storybook",
    "build": "yarn build:js && yarn build:css",
    "dev:css": "cd semantic-ui && gulp watch",
    "dev": "nodemon --exec babel-node server/express",
    "docker:build": "docker build -t nextjs-enterprise-boilerplate .",
    "docker:run": "docker run --rm -it -p 3001:3001 -e 'PORT=3001' -e 'API_URL=https://API_URL.com' nextjs-enterprise-boilerplate",
    "heroku-bd": "heroku container:push web && heroku container:release web",
    "lint:js": "eslint . --ext .js,.md --fix",
    "lint:semantic": "stylelint 'semantic-ui/src/site/**/*' --syntax less --fix",
    "lint:styles": "stylelint 'src/**/*.scss' --fix ",
    "lint": "yarn lint:js && yarn lint:styles && yarn lint:semantic",
    "start": "cross-env NODE_ENV=production node server/express",
    "storybook": "yarn buildstorybook && start-storybook -p 6006",
    "test:coverage": "jest --coverage",
    "test:selenium": "selenium-side-runner DHQ.side -c 'browserName=chrome'",
    "test:watch": "jest --watch",
    "test": "jest && yarn test:selenium",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "engines": {
    "node": "10.x"
  },
  "dependencies": {
    "@segment/snippet": "^4.4.0",
    "@sentry/browser": "^5.4.3",
    "@yolkai/next-routes": "^3.0.0",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "algoliasearch": "^3.33.0",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "cluster": "^0.7.7",
    "compression": "^1.7.4",
    "cross-env": "^5.2.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-redirect": "^1.2.2",
    "helmet": "^3.18.0",
    "hpp": "^0.2.2",
    "isomorphic-unfetch": "^3.0.0",
    "logrocket": "^1.0.1",
    "morgan": "^1.9.1",
    "next": "^8.1.0",
    "next-compose-plugins": "^2.2.0",
    "next-images": "^1.1.2",
    "next-size": "^2.0.2",
    "nocache": "^2.1.0",
    "path": "^0.12.7",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "semantic-ui-react": "^0.87.2",
    "url": "^0.11.0",
    "snyk": "^1.189.0"
  },
  "devDependencies": {
    "@babel/node": "^7.4.5",
    "babel-jest": "^24.8.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "chromedriver": "^75.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.0.1",
    "eslint-config-fixt": "git+https://github.com/fixt/eslint-config-fixt.git#v1.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-simple-import-sort": "^4.0.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.5.0",
    "jest": "^24.8.0",
    "jest-cucumber": "^2.0.11",
    "nodemon": "^1.19.1",
    "selenium-side-runner": "^3.11.0",
    "stylelint": "^10.1.0",
    "stylelint-a11y": "^1.2.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-order": "^3.0.0",
    "stylelint-scss": "^3.8.0"
  },
  "peerDependencies": {
    "redis": "^2.8.0"
  },
  "snyk": true
}
